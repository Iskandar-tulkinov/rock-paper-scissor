{
  "type": "module",
  "name": "task-management-app",
  "version": "1.0.0",
  "description": "Enjoy Your Time !",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npx tailwindcss -i ./assets/css/style.css -o ./assets/css/output.css --watch",
    "lint": "eslint --ext .html,.js .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.3.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.html": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.css": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "uuid": "^9.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Iskandar-tulkinov/rock-paper-scissor.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/Iskandar-tulkinov/rock-paper-scissor/issues"
  },
  "homepage": "https://github.com/Iskandar-tulkinov/rock-paper-scissor#readme"
}
